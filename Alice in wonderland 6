import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner Sc =new Scanner(System.in);
        int n=Sc.nextInt();
        System.out.println("Bird said:" +n);
        int path=(n/10)+(n%10);
        System.out.println("Alice must go in path-" +path);
    }
}
